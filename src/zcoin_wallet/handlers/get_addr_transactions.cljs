(def url "https://insight.zcoin.io/insight-api-zcoin/txs")

(defn fetch [addr]
  ;; TODO: fetch from pagination
  (prn :fetch-txs addr)
  (go (let [response (<! (http/get url
                                   {:with-credentials? false
                                    :query-params {:address addr}}))]
        (prn :status (:status response))
        (def data (:body response))
        (prn :body (:body response)))))


(defn get-transactions [addr]
  (for [tx   mock-txs
        v    (get tx :vout)
        :let [txid        (get tx :txid)
              value       (get v :value)
              spentTxId   (get v :spentTxId)
              out-address (get-in v [:scriptPubKey :addresses 0])]
        :when (and (= out-address addr))]

    {:txid txid
     :value value}))

     ;; TODO: use this if we want to display spent/unspent flag
     ;; :spentTxId spentTxId}))

     ;; TODO: do we need these addrs
     ;; Get source address
     ;; :address out-address}))

(defn get-unspent [addr txs]
  (for [tx   txs
        v    (get tx :vout)
        :let [txid        (get tx :txid)
              index       (get v :n)
              value       (-> v
                              (get :value)
                              (js/parseFloat)
                              (* 1e8))
              spentTxId   (get v :spentTxId)
              out-address (get-in v [:scriptPubKey :addresses 0])
              script      (get-in v [:scriptPubKey :hex])]
        :when (and (= out-address addr)
                   (nil? spentTxId))]

    {:txid txid
     :index index
     :amount value
     :address out-address
     :script script}))

